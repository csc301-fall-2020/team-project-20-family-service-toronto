{
  "expo": {
    "name": "Family Services Toronto",
    "slug": "FamilyServicesToronto",
    "description": "This app is to make it easier for seniors to use common functionality of their phone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.familyservicestoronto.FSTApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.familyservicestoronto.FSTApp"
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
